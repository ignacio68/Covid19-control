{
  "name": "distance-control",
  "version": "0.0.4",
  "description": "A native application built with NativeScript-Vue",
  "author": "Ignacio Lopez-Amor Pinillos <ignaciolopezamor@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ignacio68/distance-control.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Vue",
    "Scss",
    "Android",
    "iOS",
    "geolocation"
  ],
  "nativescript": {
    "id": "com.distancecontrol.app",
    "templateVersion": "v2",
    "tns-ios": {
      "version": "6.5.0"
    },
    "tns-android": {
      "version": "6.5.0"
    }
  },
  "scripts": {
    "dev": "tns debug android",
    "devan": "tns debug android --no-hmr --env.snapshot --env.uglify",
    "devin": "tns debug ios --no-hmr --env.uglify",
    "runan": "tns run android --no-hmr --env.snapshot --env.uglify",
    "un": "adb uninstall com.distancecontrol.app",
    "tslint": "tslint 'app/**/*.ts'",
    "ci.tslint": "npm i && tslint 'app/**/*.ts'",
    "test": "karma start --single-run"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.11.0",
    "@nativescript/theme": "^2.3.3",
    "@vue/devtools": "^5.3.3",
    "d3-ease": "^1.0.6",
    "http2": "^3.3.7",
    "nanoid": "^3.1.12",
    "nativescript-bitmap-factory": "^1.8.1",
    "nativescript-geolocation": "^5.1.0",
    "nativescript-mapbox": "^5.0.1",
    "nativescript-material-bottomsheet": "^3.2.19",
    "nativescript-material-button": "^3.2.19",
    "nativescript-material-floatingactionbutton": "^3.2.19",
    "nativescript-material-slider": "^3.2.19",
    "nativescript-material-textfield": "^3.2.19",
    "nativescript-plugin-firebase": "^10.5.2",
    "nativescript-secure-storage": "^2.6.1",
    "nativescript-socketio": "^3.3.1",
    "nativescript-toasty": "^3.0.0-alpha.2",
    "nativescript-ui-autocomplete": "^6.0.1",
    "nativescript-ui-sidedrawer": "^8.0.1",
    "nativescript-unit-test-runner": "^0.7.0",
    "nativescript-vue": "^2.7.1",
    "nativescript-vue-devtools": "^1.4.0",
    "nativescript-vue-navigator": "^1.2.0",
    "tns-core-modules": "^6.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@types/d3-ease": "^1.0.9",
    "@types/node": "^14.0.27",
    "@types/qunit": "^2.9.1",
    "@vue/composition-api": "^1.0.0-beta.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "got": "^11.5.1",
    "karma": "^6.3.14",
    "karma-nativescript-launcher": "0.4.0",
    "karma-qunit": "^4.1.1",
    "karma-webpack": "^4.0.2",
    "keyv": "^4.0.1",
    "nativescript-dev-webpack": "^1.5.1",
    "nativescript-vue-template-compiler": "^2.7.1",
    "nativescript-worker-loader": "~0.12.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "qunit": "^2.10.1",
    "tns-platform-declarations": "^6.5.12",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "vue": "^2.6.11",
    "vue-i18n": "^8.20.0",
    "vue-loader": "^15.9.3"
  }
}
